{
  "name": "control-am-web",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "eslint": "npx eslint src --fix",
    "prettier": "npx prettier \"src/**/*.{ts,html,css,scss}\" --write",
    "check": "npm run openapi & npm run prettier && npm run eslint",
    "openapi": "ng-openapi-gen",
    "start": "npm run check && ng serve",
    "build": "npm run check && ng build",
    "watch": "npm run check && ng build --watch --configuration development",
    "test": "ng test"
  },
  "build": {
    "builder": "@angular-devkit/build-angular:browser",
    "options": {
      "outputPath": "dist/motional-ui",
      "index": "src/index.html",
      "main": "src/main.ts",
      "polyfills": "src/polyfills.ts",
      "tsConfig": "src/tsconfig.app.json",
      "assets": [
        "src/assets"
      ],
      "styles": [
        "src/styles.css"
      ],
      "scripts": []
    }
  },
  "postinstall": "ng build",
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.8",
    "@angular/cdk": "^18.2.9",
    "@angular/common": "^18.2.8",
    "@angular/compiler": "^18.2.8",
    "@angular/core": "^18.2.8",
    "@angular/forms": "^18.2.8",
    "@angular/material": "^18.2.9",
    "@angular/material-moment-adapter": "^18.2.9",
    "@angular/platform-browser": "^18.2.8",
    "@angular/platform-browser-dynamic": "^18.2.8",
    "@angular/router": "^18.2.8",
    "angular-eslint": "^18.4.0",
    "control-am-web": "file:",
    "crypto-js": "^4.2.0",
    "moment": "^2.30.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.9",
    "@angular-eslint/builder": "^18.4.0",
    "@angular-eslint/eslint-plugin": "^18.4.0",
    "@angular-eslint/eslint-plugin-template": "^18.4.0",
    "@angular-eslint/schematics": "^18.4.0",
    "@angular-eslint/template-parser": "^18.4.0",
    "@angular-eslint/utils": "^18.4.0",
    "@angular/cli": "^18.2.9",
    "@angular/compiler-cli": "^18.2.8",
    "@eslint/js": "^9.13.0",
    "@types/crypto-js": "^4.2.2",
    "@types/jasmine": "^5.1.4",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jasmine-core": "^5.4.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "ng-openapi-gen": "^0.52.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.11.0"
  }
}
